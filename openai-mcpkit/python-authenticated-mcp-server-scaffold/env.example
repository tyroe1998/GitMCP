# Copy to .env and fill in values

# OpenAI
# Secret used by the server when calling OpenAI APIs from the example tools.
OPENAI_API_KEY=
# Identifier of the OpenAI Vector Store that powers transcript search/fetch.
VECTOR_STORE_ID=

# Auth0
# Base URL of your Auth0 (or other OIDC) tenant, e.g. https://tenant.us.auth0.com.
AUTH0_ISSUER=

# Local server
# Port the local MCP server binds to during development.
PORT=8788
# External URL the MCP server is reachable at, set to your ngrok url or production url.
RESOURCE_SERVER_URL=
# Comma-separated list of OAuth audiences the server accepts on incoming tokens. This should match the audience you've set as default in Auth0.
JWT_AUDIENCES=
